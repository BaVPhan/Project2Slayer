<h2>Create a new hero!</h2>
<ul>
<%= form_for [@hero] do |f| %>
  <li>
    <%= f.label :name %>
    <%= f.text_field :name %>
  </li>
  <br>
  <li>
    <%= f.label :hero_strength %>
    <%= f.text_field :weapon_strength %>
  </li>
  <br>
  <li>
    <%= f.label :photo %>
    <%= f.text_field :img_url %>
  </li>
  <br>
    <%= f.submit %>
  <% end %>
</ul>
